{
  "name": "@gratonite/web",
  "version": "0.1.0",
  "private": true,
  "description": "Gratonite web app",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "e2e": "PLAYWRIGHT_BROWSERS_PATH=\"$PWD/.cache/ms-playwright\" playwright test",
    "e2e:visual": "PLAYWRIGHT_BROWSERS_PATH=\"$PWD/.cache/ms-playwright\" playwright test tests/e2e/visual.spec.ts --update-snapshots",
    "e2e:headed": "PLAYWRIGHT_BROWSERS_PATH=\"$PWD/.cache/ms-playwright\" playwright test --headed",
    "e2e:ui": "PLAYWRIGHT_BROWSERS_PATH=\"$PWD/.cache/ms-playwright\" playwright test --ui",
    "e2e:install": "PLAYWRIGHT_BROWSERS_PATH=\"$PWD/.cache/ms-playwright\" playwright install chromium"
  },
  "dependencies": {
    "@gratonite/profile-resolver": "workspace:*",
    "@gratonite/types": "workspace:*",
    "@tanstack/react-query": "^5.60.0",
    "@tanstack/react-virtual": "^3.13.2",
    "livekit-client": "^2.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.28.0",
    "socket.io-client": "^4.8.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.3",
    "vite": "^5.4.8"
  }
}
